{
  "name": "then-busboy",
  "description": "Promise-based wrapper around Busboy. Processes multipart/form-data request body and returns it in a single object.",
  "version": "5.1.3",
  "author": "Nick K. <io@octetstream.me>",
  "license": "MIT",
  "repository": "octet-stream/then-busboy",
  "type": "module",
  "types": "@type/index.d.ts",
  "main": "./lib/index.js",
  "exports": {
    "import": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "require": {
      "types": "./lib/index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "multipart",
    "promise",
    "async",
    "formdata",
    "form-data",
    "file",
    "upload",
    "form-data",
    "body"
  ],
  "scripts": {
    "eslint": "eslint src/**/*.ts",
    "staged": "lint-staged",
    "coverage": "c8 pnpm test",
    "ci": "c8 pnpm test && c8 report --reporter=json",
    "build:types": "tsc --project tsconfig.d.ts.json",
    "build": "del-cli lib && tsup",
    "test": "cross-env NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm\" ava",
    "prepare": "npx is-in-ci || husky install"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "nanoid"
      ]
    }
  },
  "dependencies": {
    "@types/busboy": "1.5.2",
    "busboy": "1.6.0",
    "formdata-node": "6.0.3",
    "http-errors": "2.0.0",
    "lodash.merge": "4.6.2",
    "nanoid": "3.3.4",
    "object-deep-from-entries": "0.4.0",
    "typed-emitter": "2.1.0"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "5.0.0",
    "@types/http-errors": "2.0.3",
    "@types/lodash.merge": "4.6.8",
    "@types/node": "20.8.10",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "ava": "5.3.1",
    "c8": "8.0.1",
    "codecov": "3.8.3",
    "cross-env": "7.0.3",
    "del-cli": "5.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-ava": "12.0.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "form-data-encoder": "4.0.2",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "lowercase-keys": "3.0.0",
    "node-fetch": "3.3.2",
    "pinst": "3.0.0",
    "promise-fs": "2.1.1",
    "proxyquire": "2.1.3",
    "sinon": "14.0.0",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  }
}
